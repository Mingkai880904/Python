#!/usr/bin/python
# -*- coding: UTF-8 -*-
 
a = int(raw_input("請輸入一個數字:\n"))
x = str(a)
flag = True
 
for i in range(len(x)/2):
    if x[i] != x[-i - 1]:
        flag = False
        break
        
if flag:
    print "%d 是一個回文數!" % a
else:
    print "%d 不是一個回文數!" % a

continue

digits = (1, 2, 3, 4)
for i in digits:
    ii = i*100
    for j in digits:
        if j==i:
            continue
        jj = j*10
        for k in digits:
            if k==i or k==j:
                continue
            print(ii+jj+k)

